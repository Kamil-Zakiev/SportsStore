@model string[]

@Html.ActionLink("All", "List", "Product")

@foreach (var category in Model)
{
    @Html.RouteLink(category, new
{
    controller = "Product",
    action = "List",
    category = category,
    page = 1
},
        new
        {
            @class = category == ViewBag.SelectedCategory ? "selected" : null
        })
}